{
  "name": "head-shakers",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:fresh": "npm run db:reset && npm run db:migrate && npm run db:seed",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:reset": "npx tsx src/lib/db/scripts/reset-db.ts",
    "db:seed": "npx tsx src/lib/db/scripts/seed.ts",
    "dev": "next dev --turbopack",
    "format": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "next-typesafe-url": "next-typesafe-url --filename route-type --pageExtensions ts,tsx",
    "start": "next start",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "vitest run --coverage && start ./coverage/index.html",
    "test:coverage:watch": "vitest --coverage",
    "test:db": "vitest run tests/database",
    "test:db:reset": "bun run db:reset && bun run db:migrate",
    "test:db:setup": "bun run db:migrate",
    "test:e2e": "vitest run tests/e2e",
    "test:integration": "vitest run tests/integration",
    "test:load": "vitest run tests/performance --reporter=verbose",
    "test:performance": "vitest run tests/performance",
    "test:security": "vitest run tests/security",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
